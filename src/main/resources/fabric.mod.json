{
  "schemaVersion": 1,
  "id": "pmpp",
  "version": "${version}",

  "name": "PMPP",
  "description": "Piston Message and Presence Protocol (blame kat)",
  "authors": [
    "LemmaEOF"
  ],
  "contact": {
    "homepage": "https://lemmaeof.gay",
    "sources": "https://github.com/LemmaEOF/pmpp"
  },

  "license": "FAFOL-0.2",
  "icon": "assets/pmpp/icon.png",

